    <validationRules>
        <fullName>Prevent_update_after_closed</fullName>
        <active>true</active>
        <description>If planning version is closed  then if somebody trying to change data entries start date and end date manually.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Version_Status__c , &apos;Closed&apos;),
    OR(
       ISCHANGED( Data_Entry_Start_Date__c ),
       ISCHANGED( Data_Entry_Closed_Date__c )
    )
)</errorConditionFormula>
        <errorMessage>you can not change version date with closed version status</errorMessage>
    </validationRules>
